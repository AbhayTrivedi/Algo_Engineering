#include<iostream>
using namespace std;

int main(){
    int a,b;
    cout<<"Enter the Starting Number a: ";
    cin>>a;
    cout<<"Enter the End Number b: ";
    cin>>b;
    cout<<"Prime Numbers Between Given Numbers are: "<<endl;
    for(int num=a; num<=b;num++)
    {
        int i;
        for (i=2;i<num;i++)
        {
            if (num%i==0)
            {
                break;
            }
        }
        if (i==num)
        {
            cout<<num<<endl;
        }

    }
       return 0;
}
